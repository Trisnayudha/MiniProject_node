<div class="container mt-3">
    <div class="row">
        <div class="col-md-8">
            <h2>
                Detail Contact
            </h2>
            <div class="card" style="width: 18rem;">
                <div class="card-body">
                    <h5 class="card-title"><%= contact.nama%></h5>
                    <h6 class="card-subtitle mb-2 text-muted"><%= contact.nohp%></h6>
                    <p class="card-text"><%= contact.alamat%></p>
                    <a href="/contact/delete/<%= contact.nama%>" class="btn btn-danger badge"
                        onclick="return confirm('Delete entry?')">Hapus</a>
                    <a href="/contact/edit/<%= contact.nama  %> " class="btn btn-primary badge">Update</a>
                </div>
            </div>
        </div>
    </div>
</div>